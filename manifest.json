{
    "manifest_version": 2,
    "name": "EasyTest",
    "description": "简化输入操作",
    "version": "1.0",
    "icons": {
        "128" : "icon.png"
    },
    "permissions": [
        "tabs", 
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
        {
            "js":[
                "data.js",
                "du.user.js"
            ],
            "matches":["<all_urls>"]
        }
    ],
    "browser_action":{
        "default_icon": "icon.png",
        "default_title": "这是一个简化输入操作的插件"
    }
}